! ------------------------------------------------------- Rofi
rofi.terminal: urxvt
rofi.lines:    3

rofi.width:    20
rofi.padding:  30

rofi.eh:       2
rofi.opacity:  90
rofi.bw:       2

rofi.bc:       #ebdbb2
rofi.bg:       #ebdbb2
rofi.fg:       #282828

rofi.hlbg:     #8ec07c
rofi.hlfg:     #282828

rofi.font:     Roboto 14

! ------------------------------------------------------- URxvt
URxvt*termName:          rxvt-256color
URxvt*xftAntialias:      true

URxvt*cursorColor:       #fbf1c7
URxvt*colorUL:           #366060
URxvt*underlineColor:    #dfaf8f

URxvt*letterSpace:       0

URxvt*transparent:       false
URxvt*shading:           25

URxvt.perl-ext-common:   default,matcher
URxvt.url-launcher:      /usr/bin/chromium-browser
URxvt.matcher.button:    1

URxvt*loginShell:        true
URxvt*cursorBlink:       true
URxvt*pointerBlank:      true
URxvt*pointerBlankDelay: 2
URxvt*scrollBar:         false
URxvt*secondaryScroll:   true
URxvt*borderless:        true
URxvt.internalBorder:    10

URxvt.geometry: 90x55
